{
  "extends": ["config:base", ":semanticCommits"],
  "branchPrefix": "rv8-",
  "hashedBranchLength": 20,
  "separateMinorPatch": true,
  "platformAutomerge": true,
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "updateTypes": ["patch"],
      "labels": ["automerge"],
      "automerge": true
    }
  ]
}
